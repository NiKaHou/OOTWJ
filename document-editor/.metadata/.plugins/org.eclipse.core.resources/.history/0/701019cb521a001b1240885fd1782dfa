package strategy_pattern;

import javafx.scene.layout.AnchorPane;
import javafx.stage.Stage;

public class WindowSizeFlexible implements WindowSize{

	@Override
	public void setWindowSize(Stage stage) {
		AnchorPane anchorPane = (AnchorPane) stage.getScene().lookup("#mainPane");
		anchorPane.setPrefHeight(0);
		System.out.println();
	}
	
}
