package composite_pattern;

import java.util.Map;

public abstract class MenuBarComponent {
	public void add(String title, MenuBarComponent component){
		throw new UnsupportedOperationException();
	}
	public void remove(String title) throws UnsupportedOperationException {
		throw new UnsupportedOperationException();
	}
	public boolean hasChild() {
		throw new UnsupportedOperationException();
	}
	public Map<String, MenuBarComponent> getChild() {
		throw new UnsupportedOperationException();
	}
	abstract public String getTitle();
	abstract public String getType();
}
